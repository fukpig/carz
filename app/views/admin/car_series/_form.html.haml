= form_for @series, url: admin_cars_make_car_series_index_path(params[:cars_make_id]), html: { method: :post } do |f|
  - if @series.errors.any?
    #error_explanation
      %h2= "#{pluralize(@make.errors.count, "error")} prohibited this car from being saved:"
      %ul
        - @series.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :name
    = f.text_field :name 
  .actions
    = f.submit 'Save'