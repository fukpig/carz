.box-body.table-responsive
  %a{:href => new_admin_cars_make_car_series_path}
    %button.btn.btn-info New series
  %br/
  %table#example2.table.table-bordered.table-striped
    %thead
      %tr
        %th Series
        %th Actions
    %tbody
      - @series.each do |s|
        %tr
          %td= s.name
          %td 
            = link_to 'Edit', edit_admin_cars_make_car_series_path(params[:cars_make_id],s)
            = link_to "Delete", admin_cars_make_car_series_path(params[:cars_make_id], s), method: :delete, data: { confirm: 'Are you certain you want to delete this?' }

%script{:src => "//maxcdn.bootstrapcdn.com/bootstrap/3.3.1/js/bootstrap.min.js", :type => "text/javascript"}
%script{:src => "//code.jquery.com/ui/1.11.1/jquery-ui.min.js", :type => "text/javascript"}
%script{:src => "//cdnjs.cloudflare.com/ajax/libs/raphael/2.1.0/raphael-min.js"}