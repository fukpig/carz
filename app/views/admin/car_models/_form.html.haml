= form_for @model, url: admin_cars_make_car_models_path(params[:cars_make_id]), html: { method: :post } do |f|
  - if @model.errors.any?
    #error_explanation
      %h2= "#{pluralize(@make.errors.count, "error")} prohibited this car from being saved:"
      %ul
        - @model.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :name
    = f.text_field :name 
  .field
    = f.label :series
    = select("cars_models", "series_id", @series.collect {|p| [ p.name, p.id ] }, { include_blank: true })
  .actions
    = f.submit 'Save'